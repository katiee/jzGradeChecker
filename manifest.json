{
  "manifest_version": 2,
  "name": "jzGradeChecker",
  "description": "汕头市金山中学学生成绩查询界面优化插件。",
  "version": "0.1",
  "update_url": "http://github.phy25.com/jzGradeChecker/chrome-extension/updates.xml",
  "permissions": [
  
  ],
  "content_scripts": [
    {
      "matches": [
        "http://jszx.stedu.net/jszxcj/*.htm",
        "http://www.stjszx.net/jszxcj/*.htm",
        "http://stjszx.net/jszxcj/*.htm"
      ],
      "css": ["bootstrap/css/bootstrap.min.css", "index.css"],
      "js": ["jquery-1.8.3.min.js", "commonData.js", "index.js"],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "http://jszx.stedu.net/jszxcj/*.asp",
        "http://www.stjszx.net/jszxcj/*.asp",
        "http://stjszx.net/jszxcj/*.asp"
      ],
      "css": ["bootstrap/css/bootstrap.min.css", "result.css"],
      "js": ["jquery-1.8.3.min.js", "commonData.js", "result.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}